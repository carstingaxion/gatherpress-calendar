{
	"$schema": "https://schemas.wp.org/trunk/block.json", 
	"apiVersion": 3, 
	"name": "gatherpress/query-map", 
	"version": "0.1.0", 
	"title": "Query Map",
	"category": "gatherpress", 
	"icon": "location-alt", 
	"description": "Display query loop posts on an interactive OpenStreetMap based on their geo coordinates.", 
	"ancestor": ["core/query"], 
	"usesContext": [ "queryId", "query", "queryContext", "displayLayout", "templateSlug", "previewPostType" ],
	"attributes": { 
		"mapHeight": { 
			"type": "string", 
			"default": "500px" 
		},
		"defaultZoom": {
			"type": "number", 
			"default": 10 
		}
	},
	"example": {}, 
	"supports": {
		"reusable": false, 
		"html": false, 
		"align": true, 
		"alignWide": true, 
		"customClassName": true
	},
	"textdomain": "gatherpress-calendar", 
	"editorScript": "file:./index.js", 
	"editorStyle": "file:./index.css", 
	"style": "file:./style-index.css", 
	"viewScript": "file:./view.js", 
	"render": "file:./render.php" 
}